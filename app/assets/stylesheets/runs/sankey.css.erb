.sankey {
  height: 10000px;
  margin: 0 10px 0 10px;
}

.sankey .node rect {
  cursor: move;
  fill-opacity: .9;
  shape-rendering: crispEdges;
}

.sankey .node text {
  pointer-events: none;
  text-shadow: 0 1px 0 #fff;
}

.sankey .link {
  fill: none;
  stroke-opacity: .8;
}

.sankey .link:hover {
  stroke-opacity: 1;
}
